		$(".view_on_map").click(function(){
			var id = $(this).attr("href").replace("#", "");

			var courts = markers[id];
			var minLat = 999;
			var maxLat = -999;
			var minLng = 999;
			var maxLng = -999;
			for(var i in courts) {
				var latlng = courts[i].getPosition();
				if(latlng.lat() < minLat) {
					minLat = latlng.lat();
				}
				if(latlng.lat() > maxLat) {
					maxLat = latlng.lat();
				}
				if(latlng.lng() < minLng) {
					minLng = latlng.lng();
				}
				if(latlng.lng() > maxLng) {
					maxLng = latlng.lng();
				}
			}

			var bounds = new google.maps.LatLngBounds(new google.maps.LatLng(minLat - 0.0001, minLng - 0.0001), new google.maps.LatLng(maxLat + 0.0001, maxLng + 0.0001));
			map.fitBounds(bounds);

			return false;
		});
	});
