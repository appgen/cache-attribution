    $('body.mmr').ready(function() {
        $.getScript('/assets/489C82E9-3E7D-4332-BAE7-ABA6656A1E67')
            .done(function() {
                $('.cpr-link').each(function() {
                    $(this).data('url', $(this).attr('href'));
                    $(this).attr('href', '#cpr-colorbox');
                    $(this).click(function() {
                        $('#cpr-colorbox-link').attr('href', $(this).data('url')).text($(this).data('url'));
                        $(this).colorbox({inline:true, width:'50%', speed:200, opacity:0.5});
                    });
                });
            });
    });
    $('a.print-link').each(function() {
          $(this).click(function() { window.print(); return false; });
    });
});
