<script language="javascript">
function doRadGoogleSearch()
{
	var objTxtBox = $find('doecontrol_topcontainer_a_regionbanner_ctrlRadTextBox');
    var searchtxt = objTxtBox.get_value();
    
    if (searchtxt == "")
    {
        return;
    }
    searchtxt = searchtxt.replace(/~|`|!|@|#|\$|%|\^|&|\*/gi,'');
	searchtxt = searchtxt.replace(/\(/gi,'');
	searchtxt = searchtxt.replace(/\)/gi,'');	
	searchtxt = searchtxt.replace(/\=/gi,'');
	searchtxt = searchtxt.replace(/\+/gi,'');
	searchtxt = searchtxt.replace(/\[/gi,'');
	searchtxt = searchtxt.replace(/\]/gi,'');
	searchtxt = searchtxt.replace(/\{/gi,'');
	searchtxt = searchtxt.replace(/\}/gi,'');
	window.location.href = 'http://search.nycenet.edu/search?q=' + searchtxt + '&btnG=DOE+Search&site=default_collection&client=default_frontend&output=xml_no_dtd&proxystylesheet=default_frontend';
}
function onkeydownG(e)
{
    if  (e.keyCode == 13) 
	{
		doRadGoogleSearch();
		return false;
	}
	return true;
}
