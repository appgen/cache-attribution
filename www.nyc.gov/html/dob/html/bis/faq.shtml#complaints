				<!-- START of THIRD column -->
				<td align="left" valign="top" width="220" id="col3_content">
					
						  <span class="module_bodytext">
    <script language="JavaScript" type="text/JavaScript">
<!--


//Slide code

var curSlide;
var curTime;

var slides = new Array();

slides[slides.length] = "../../images/scrolling news/scrolling_news_background.gif|Tickets on Sale Now|<center>Tickets on Sale Now<p></center>|<a href='http://www.nyc.gov/html/dob/html/news/construction_safety_week.shtml'><center>Click here for tickets to the 2013 Build Safe Live Safe Conference! During this full-day event, Department engineers, architects and construction experts will outline industry trends and safe construction practices.</center></a>";

