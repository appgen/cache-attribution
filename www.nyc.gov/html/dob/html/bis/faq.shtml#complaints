slides[slides.length] = "../../images/scrolling news/scrolling_news_background.gif|Special Inspection Agency Registration Deadline Extended|<center>Special Inspection Agency Registration Deadline Extended<p></center>|<a href='http://www.nyc.gov/html/dob/html/development/special_insp_overview.shtml'><center>The Department has extended the deadline to register as a special inspection agency to May 13, 2013. Click here to learn more about how to register and the extension.</center></a>";

var slidesLoad = new Array();
for (x=0; x<slides.length; x++) {
	var tmpImgSplit = slides[x].split("|");
	slidesLoad[x] = new Image();
	slidesLoad[x].src = tmpImgSplit[0];



}



function switchSlide(slideNum) {

	var tmpSplit = slides[slideNum].split("|");

	if (document.images.slide_main.filters) {
		//document.images.slide_main.style.filter="blendTrans(duration=.5)";
		document.images.slide_main.filters.blendTrans.Stop();
	    document.images.slide_main.filters.blendTrans(duration=.5).Apply();
	}

	document.slide_main.src = slidesLoad[slideNum].src;  //tmpSplit[0];
	document.slide_main.alt = tmpSplit[1];
	document.getElementById("boldtitle").innerHTML = tmpSplit[2];
	document.getElementById("slidetext").innerHTML = tmpSplit[3];

	if (document.images.slide_main.filters) {
	    document.images.slide_main.filters.blendTrans.Play();
	}

	curSlide = slideNum;
}

function nextSlide() {
	if (curSlide >= (slides.length -1)) {
		switchSlide(0);
	} else {
		switchSlide(curSlide+1);
	}
	setCurTime();
}

function prevSlide() {
	if (curSlide <= 0) {
		switchSlide((slides.length -1));
	} else {
		switchSlide(curSlide-1);
	}
	setCurTime();
}

function setCurTime() {
	curTime = new Date().getTime();
}

function setTimer() {
	setTimeout("compareTime()",500);
}

function compareTime() {
	if (document.images.slide_main.filters) {
		if (document.images.slide_main.filters.blendTrans.status == 0) {
			var tmpImgSplit = slides[curSlide].split("|");
			document.slide_main.src = tmpImgSplit[0];
		}
	}

	var tmpTime = new Date().getTime();
	if ((tmpTime - curTime) >= 15000) {
		nextSlide();
		setCurTime();
	}

	setTimer();
}

function slideInit() {
	switchSlide(0);
	setCurTime();
	setTimer();
}

window.onload = slideInit;


//-->
</script><div style="position: absolute; left: 780px; top: 158px; width: 179px;">
